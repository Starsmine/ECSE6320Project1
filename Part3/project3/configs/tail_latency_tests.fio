[global]
direct=1
time_based=1
runtime=300  # Longer runtime for better tail latency characterization
ramp_time=5
norandommap=1
randrepeat=0
percentile_list=50:95:99:99.9:99.99:99.999
lat_percentile=1
size=4G
directory=/mnt/g
ioengine=libaio
filename=${DIRECTORY}/fio_testfile
bs=4k
rw=randread

[mid-qd]
iodepth=16
numjobs=1

[high-qd]
iodepth=128
numjobs=1